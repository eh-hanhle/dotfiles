{
  "coc.source.file.ignorePatterns": [
    "node_modules/*"
  ],
  "coc.preferences.floatActions": false,
  "diagnostic.messageTarget": "echo",
  "eslint.options": {
    "ignorePattern": [
      "node_modules/*",
      "*.bs.js"
    ]
  },
  "eslint.run": "onSave",
  "java.jdt.ls.vmargs": "-javaagent:/Users/eh/.m2/repository/org/projectlombok/lombok/1.18.12/lombok-1.18.12.jar -Xbootclasspath/a:/Users/eh/.m2/repository/org/projectlombok/lombok/1.18.12/lombok-1.18.12.jar",
  "java.maven.downloadSources": true,
  "java.completion.filteredTypes": [
    "java.awt.*",
    "com.sun.*",
    "antlr.*"
  ],
  "prettier.eslintIntegration": true,
  "python.jediEnabled": false,
  "reason_language_server.reloadOnChange": true,
  "solargraph.autoformat": true,
  "solargraph.checkGemVersion": false,
  "solargraph.diagnostics": true,
  "solargraph.formatting": true,
  "sql.formatOptions":{"uppercase": true},
  "suggest.timeout": 6000,
  "eslint.workingDirectories":["./packages/core"],
  "eslint.autoFixOnSave":false,
  "workspace.ignoredFiletypes": [
    "log",
    "txt",
    "help",
    "class",
    "jar"
  ],
  "yank.highlight.enable": false
}
